{
  "manifest_version": 2,
  "name": "QRcode",
  "description": "Chrome plugin with Vue",
  "version": "0.0.1",
  "options_page": "options.html",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "permissions": ["tabs"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
